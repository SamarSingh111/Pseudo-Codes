# Pseudo code (leet code 20)

function isValid(s):
    stack = empty list
    for char in s:
        if char in ['(', '[', '{']:
            push char to stack
        else:
            if stack is empty:
                return false
            top = pop from stack
            if char == ')' and top != '(':
                return false
            if char == ']' and top != '[':
                return false
            if char == '}' and top != '{':
                return false
    return stack is empty
